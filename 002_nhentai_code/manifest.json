{
  "manifest_version": 2,

  "name": "nhentai code",
  "description": "Open link with some code",
  "version": "1.0",
  "background" : { 
    "scripts": [
      "background.js"
      ],
    "persistent": false
    },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "images/logo.png",
        "48": "images/logo.png"
    }
  },
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "permissions": [ 
    "contextMenus", 
    "tabs",  
    "activeTab" 
  ],
  "author": "VuTrungTruc-Slimetan"
}